{
  "extends": "next/core-web-vitals",
  "rules": { 
    "quotes": ["error", "double"],
    "no-duplicate-imports": 0,
    "eol-last": ["error", "always"],
    "import/no-unresolved": 2,
    "no-unused-expressions": 0,
      "no-undefined": 0,
    "no-use-before-define": 0,
        "max-lines": ["warn", {"max": 200, "skipBlankLines": true, "skipComments": true}]
  }
}
